{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "types": ["express", "multer"],
    "skipLibCheck": true,
    "moduleResolution": "node"
  },
  "include": ["src/**/*.ts" , "src/server.ts"],
  "exclude": [
    "node_modules",
    "../electron",
    "../node_modules/@types/electron"
  ]
}
